{
  "extends": [
    "eslint:recommended"
  ],
  "env": {
    "es6": true,
    "jest": true,
    "node": true
  },
  "rules": {
    "no-console": 0,
    "indent": [
      "error",
      "tab"
    ],
    "semi": [
      "error",
      "always"
    ],
    "quotes": [
      "error",
      "double"
    ]
  }
}